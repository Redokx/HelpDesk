@model Helpdesk.Models.Priorytet

@{
    ViewBag.Title = "Utwórz";
}

<h2>Utwórz nowy priorytet</h2>


@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal">
      
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        <div class="form-group">
            @Html.LabelFor(model => model.NazwaPriorytetu, htmlAttributes: new { @class = "applicationLabel col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.NazwaPriorytetu, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.NazwaPriorytetu, "", new { @class = "text-danger" })
            </div>
        </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Utwórz" class="btn btn-default" />
                <button class="btn"><a href='javascript:history.go(-1)'>Powrót</a></button>
            </div>
        </div>
    </div>
    
}



@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
